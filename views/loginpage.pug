doctype html
html
	head
		meta(charset="UTF-8")
		title="Login Google"
	body
		h1 Login Application
		p You can login below via Google
		form(action="/auth/google", method="GET")
			p Login:
				input(type="text", name="login")
			p Password:
				input(type="text" pass="password")
			button(type="submit") Login